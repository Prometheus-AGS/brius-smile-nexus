{
  "name": "migration-tool",
  "version": "1.0.0",
  "description": "Legacy Django MDW to Supabase migration tool",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "setup:db": "tsx src/setup-database.ts",
    "migrate:full": "tsx src/index.ts --type=full",
    "migrate:incremental": "tsx src/index.ts --type=incremental",
    "migrate:dry-run": "tsx src/index.ts --type=full --dry-run",
    "migrate:test": "tsx src/index.ts --type=test --limit=10"
  },
  "keywords": [
    "migration",
    "supabase",
    "django",
    "postgresql",
    "cli"
  ],
  "author": "Brius Healthcare Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.840.0",
    "@aws-sdk/client-bedrock-runtime": "^3.840.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/ora": "^3.2.0",
    "axios": "^1.10.0",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "openai": "^5.8.2",
    "ora": "^8.2.0",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  }
}
