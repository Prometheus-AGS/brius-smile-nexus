# Supabase Configuration
VITE_SUPABASE_URL=YOUR_SUPABASE_URL
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Langfuse Configuration
VITE_LANGFUSE_BASE_URL=https://langfuse.brius.com
VITE_LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key-here
VITE_LANGFUSE_SECRET_KEY=sk-lf-your-secret-key-here
VITE_LANGFUSE_HOST=https://langfuse.brius.com

# Optional Langfuse Configuration
VITE_LANGFUSE_ENABLED=true
VITE_LANGFUSE_DEBUG=false
VITE_LANGFUSE_FLUSH_INTERVAL=1000
VITE_LANGFUSE_BATCH_SIZE=10

# Mastra Data Sync Configuration
# Used by mastra-data-sync-service for threads, messages, and memory synchronization
# Note: Falls back to VITE_MASTRA_BASE_URL if not set
# VITE_MASTRA_API_URL=http://localhost:4111

# Mastra Multi-Agent Configuration
# Base URL for all Mastra agent services (default: http://localhost:3000)
VITE_MASTRA_BASE_URL=http://localhost:3000

# API key for Mastra authentication (optional for localhost development)
VITE_MASTRA_API_KEY=

# Orchestrator Agent Configuration (Primary - Intelligent Routing)
VITE_MASTRA_ORCHESTRATOR_ENDPOINT=/agents/orchestrator-agent/generate
VITE_MASTRA_ORCHESTRATOR_TIMEOUT=45000

# Business Intelligence Agent Configuration (Direct BI Access)
VITE_MASTRA_BI_ENDPOINT=/agents/business-intelligence-agent/generate
VITE_MASTRA_BI_TIMEOUT=60000

# Default Agent Configuration (Simple Queries & Fallback)
VITE_MASTRA_DEFAULT_ENDPOINT=/agents/default-agent/generate
VITE_MASTRA_DEFAULT_TIMEOUT=30000

# Multi-Agent Strategy Configuration
# Primary agent to use (orchestrator | business-intelligence | default)
VITE_MASTRA_PRIMARY_AGENT=orchestrator

# Enable automatic fallback between agents (default: true)
VITE_MASTRA_ENABLE_FALLBACK=true

# Fallback chain order (comma-separated agent names)
VITE_MASTRA_FALLBACK_CHAIN=orchestrator,business-intelligence,default

# Global Configuration
# Maximum number of retry attempts for all agents (default: 3)
VITE_MASTRA_MAX_RETRIES=3

# Health check interval in milliseconds (default: 60000)
VITE_MASTRA_HEALTH_CHECK_INTERVAL=60000

# Enable debug logging for all Mastra clients (default: false)
VITE_MASTRA_DEBUG=false

# Legacy Configuration (Maintained for Backward Compatibility)
# Agent name to connect to (default: business-intelligence) - DEPRECATED
VITE_MASTRA_AGENT_NAME=business-intelligence

# Request timeout in milliseconds (default: 30000) - DEPRECATED
VITE_MASTRA_TIMEOUT=30000
